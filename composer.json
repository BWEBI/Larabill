{
    "name": "bwebi/larabill",
    "description": "Billing package for the Laravel framework",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Chen Alon",
            "email": "chen@bwebi.co.il"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/support": "^5.5",
        "guzzlehttp/guzzle": "^6.3",
        "vlucas/phpdotenv": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "Bwebi\\Larabill\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4",
        "orchestra/testbench": "^3.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Bwebi\\Larabill\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bwebi\\Larabill\\LarabillServiceProvider"
            ],
            "aliases": {
                "Larabill": "Bwebi\\Larabill\\Facades\\LarabillFacade"
            }
        }
    }
}
